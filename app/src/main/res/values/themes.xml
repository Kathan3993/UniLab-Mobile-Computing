<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MobileComputingProject" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorSecondary">#FF9800</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#FF9800</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#FF9800</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MobileComputingProject1" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorSecondary">#FF9800</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#FF9800</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#FF9800</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MobileComputingProject.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MobileComputingProject.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MobileComputingProject.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>